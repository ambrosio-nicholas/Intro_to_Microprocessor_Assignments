' {$STAMP BS2}
' {$PBASIC 2.5}
degrees VAR Word
brads VAR Word
sine VAR Word
cosine VAR Word

DEBUG "Degrees Brads Cosine Sine", CR

FOR degrees = 0 TO 359 STEP 15
  brads = degrees ** 46733
  sine = SIN brads
  cosine = COS brads

  DEBUG " ",SDEC3 degrees, " ",SDEC3 brads, " ",SDEC3 cosine, " ",SDEC3 sine, CR

NEXT

END
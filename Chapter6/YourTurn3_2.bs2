'{$STAMP BS2}
'{$PBASIC 2.5}
index VAR Nib

OUTH = %00000000
DIRH = %11111111


PAUSE 1000
DEBUG "index OUTH ", CR,
"----- --------", CR

FOR index = 0 TO 15
  LOOKUP index, [ %11100111, %10000100, %11010011, %11010110, %10110100, %01110110, %01110111, %11000100, %11110111,%11110110, '0- 9
                  %11110101, %00110111, %01100011, %10010111, %01110011, %01110001 ], OUTH 'a-f
  DEBUG " ", DEC2 index, " ", BIN8 OUTH, CR
  PAUSE 1000
NEXT

DIRH = %00000000

END